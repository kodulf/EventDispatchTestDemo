                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="onTouch false" />

            <RadioButton
                android:id="@+id/activity_ontouch_super"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="onTouch super" />
        </RadioGroup>
    </LinearLayout>


